		<li><a href="/#">music</a></li>
		<li><a href="/#">game</a></li>
		<li><a href="/#">news</a></li>
	</ul>

	<dir class="blogs"> <h2><center> All users</center></h2>
		<% for (let i of data) { %>
		<center><div class="runDiv">
			<h3> <%= "once you learn to read you will be forever free"%> </h3>

			<a href="/blog_details?id=<%= i.id%>"> <h2><%= i.title %> </h2></a>
			<a href="/blog_details?id=<%= i.id%>"> <p><%= i.description %> </p></a>
			<p> <%= i.email %> </p>
		</center></div>
		<% } %>
	</dir>
</body>
</html>